.container {
  background: #efeff4;
}
.addressBox {
  position: relative;
  background: #fff3d1;
  margin-bottom: 20rpx;
  height: 150rpx;
}

.name {
  font-size: 16px;
  line-height: 78rpx;
  margin: 0 30rpx;
}

.name  text {
  margin-right: 20rpx;
}

.address {
  font-size: 12px;
  margin: 0 30rpx;
}

.arrowBtn {
  position: absolute;
  right: 30rpx;
  top: 44rpx;
  width: 44rpx;
  height: 44rpx;
}

.mailLine {
  width: 750rpx;
  height: 4rpx;
}

.goodslist {
  background: #fff;
}

.listBox {
  padding: 30rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  border-bottom: 1rpx solid #f5f5f5;
}

.listBox image {
  width: 200rpx;
  height: 200rpx;
}

.textCon {
  width: 460rpx;
  display: flex;
  flex-flow: column;
  justify-content: flex-start;
}

.title {
  font-size: 14px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.property {
  margin-top: 10rpx;
  font-size: 12px;
  color: #888;
}

.subCon {
  margin-top: 90rpx;
  display: flex;
  font-size: 12px;
  justify-content: space-between;
}

.other {
  background: #fff;
  padding: 0 0 0 30rpx;
}

.listItem {
  border-bottom: 1px solid #f5f5f5;
  line-height: 86rpx;
  display: flex;
  font-size: 14px;
  padding: 0 30rpx 0 0;
  justify-content: space-between;
}

.alignLeft {
  justify-content: flex-start;
}

.listLabel {
  margin-right: 40rpx;
}

.radio {
  margin-right: 30rpx;
}

.message {
  width: 500rpx;
  height: 86rpx;
}

.last {
  border-bottom: none;
  display: flex;
  justify-content: flex-end;
}

.fixFt {
background: #fff;
  height: 128rpx;
  border-top: 1px solid #eee;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 750rpx;
  display: flex;
  flex-flow: row;
  justify-content: center;
  align-items: center;
}

.fixFt button {
  width: 690rpx;
  height: 88rpx;
  line-height: 88rpx;
}