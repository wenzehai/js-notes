

.addressBox {
  position: relative;
  background: #fff3d1;
  padding: 0 30rpx;
  background-image: url(data:image/png;base64,);
  background-position: center bottom;
  background-repeat: no-repeat;
  height: 147rpx;
}

.title {
  display: block;
  font-size: 16px;
  line-height: 78rpx;
}

.address {
  display: block;
  font-size: 12px;
}

.arrowBtn {
  position: absolute;
  right: 30rpx;
  top: 40rpx;
  width: 40rpx;
  height: 40rpx;
  border: none;
  outline: none;
  background: url(data:image/png;base64,) center center;
  
}

.arrowBtn::after {
  border: none;
}
.list-box{
  padding: 30rpx;
  display: flex;
  align-items:  flex-start;
  border-top: 20rpx solid #e4e4e4;

}
.list-box-img{
  margin-right: 46rpx;

}
.list-box-ctn{
  flex-shrink: 100;

}
.list-box-title{
    font-size: 14px;
    height: 70rpx;
    width: 450rpx;
    overflow: hidden;
    line-height: 36rpx;
}
.list-box-desc{
  margin-top: 12rpx;
  font-size: 26rpx;
  color: #888;
}

.list-box-subCtn {
    display: flex;
    font-size: 12px;
    margin-top: 30rpx;
    justify-content: space-between;
}

.otherList {
  margin-left: 30rpx;
  border-top: 1rpx solid #f5f5f5;
}
.oneList {
    border-bottom: 1px solid #f5f5f5;
    line-height: 86rpx;
    display: flex;
    font-size: 14px;
    padding: 0 30rpx;
    justify-content: space-between;
}

.expressCon {
  margin-left: 30rpx;
}
.twoList {
  border-bottom: 1px solid #f5f5f5;
  line-height: 86rpx;
  display: flex;
  font-size: 14px;
  padding: 0 30rpx;
  justify-content: flex-start;
}
.message {
  line-height: 86rpx;
  /* background: red; */
  margin-left: 20rpx;
  width: 500rpx;
  height: 86rpx;
}
.lastList {
  /* border-bottom: 1px solid #f5f5f5; */
  line-height: 86rpx;
  display: flex;
  font-size: 14px;
  padding: 0 30rpx;
  justify-content: flex-end;
}

.bottom {
  background: #fff;
  height: 128rpx;
  border-top: 1px solid #eeeeee;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 750rpx;
}
.button-hover {
  background-color: red;
}
/** 添加自定义button点击态样式类**/
.other-button-hover {
  background-color: blue;
}
.submitBtn {
  margin: 20rpx 20rpx;
  display: block;
}