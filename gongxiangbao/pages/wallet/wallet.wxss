.container {
  padding-bottom: 200rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items:center;
}
.cash {
  margin-top: 20rpx;
  position: relative;
  width: 650rpx;
  height: 250rpx;
  border-radius: 10px;
  border: 1px solid #e4e4e4;
  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
  background-image: url(data:image/png;base64,);
}
.wallet {
  margin-top: 20rpx;
  position: relative;
  width: 650rpx;
  height: 250rpx;
  border-radius: 10px;
  border: 1px solid #e4e4e4;
  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
  background-image: url(data:image/png;base64,);
}
.cashBg {
  width: 750rpx;
  height: 250rpx;
}
.textBox {
  position: absolute;
  left: 50rpx;
