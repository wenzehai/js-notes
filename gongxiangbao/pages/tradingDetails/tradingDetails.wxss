.container {
  background: #fff;
}

.list {
  position: relative;
  border-bottom: 1px solid #eee;
  background: #fff;
  margin: 30rpx 30rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
}
.list:last-child {
   border-bottom: none;
}
.title {
  font-size: 16px;
  line-height: 60rpx;
}

.des {
  font-size: 12px;
  color: #d4d4d4;
  margin-bottom: 20rpx;
}

.right {
  color: #ffbe00;
  font-size: 16px;
  line-height: 112rpx;
}
